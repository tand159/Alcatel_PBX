<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.andreydom.alcatel_pbx">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityKursBase"
            android:label="@string/app_name_kurs_base"/>

        <activity android:name=".ActivityKursNetwork"
            android:label="@string/app_name_kurs_network"/>

        <activity android:name=".ActivityKonspektBase"
            android:label="@string/app_name_konspekt_base"/>

        <activity android:name=".ActivityKonspektNetwork"
            android:label="@string/app_name_konspekt_network"/>
    </application>

</manifest>